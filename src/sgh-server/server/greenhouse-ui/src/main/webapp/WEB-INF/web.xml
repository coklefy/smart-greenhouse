<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ReadInHerdServlet</servlet-name>
        <servlet-class>servlet.ReadInHerdServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewPumpLog</servlet-name>
        <servlet-class>servlet.ViewPumpLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>readConf</servlet-name>
        <servlet-class>servlet.readConf</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Readmode</servlet-name>
        <servlet-class>servlet.Readmode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadInHerdServlet</servlet-name>
        <url-pattern>/greenhouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewPumpLog</servlet-name>
        <url-pattern>/ViewPumpLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>readConf</servlet-name>
        <url-pattern>/readConf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Readmode</servlet-name>
        <url-pattern>/Readmode</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
